<div class="main-div">
    <mat-grid-list cols="12">
      <mat-grid-tile
          [colspan]="5"
          [rowspan]="6">
          <mat-card>
            <mat-card-header>
              <mat-card-title>Upload Files</mat-card-title>
            </mat-card-header>
              <app-upload-file></app-upload-file>
          </mat-card>
      </mat-grid-tile>
      <mat-grid-tile
      [colspan]="7"
      [rowspan]="6">
      <app-view-files></app-view-files>
      </mat-grid-tile>
    </mat-grid-list>
</div>